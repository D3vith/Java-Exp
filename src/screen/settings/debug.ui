  "dev_debug_hud_iteratable_toggle@iteratable_toggle_ui.iteratable_toggle_panel": {
    "$widget_text": "options.dev_enableDebugHudOverlay",
    "$option_enabled_binding_name": "#dev_debug_hud_dropdown_enabled",
    "$options": [
      {
        "@iteratable_toggle_ui.iteratable_toggle": {
          "$this_action": "#debug_hud_radio_off",
          "$next_action": "#debug_hud_radio_basic",
          "$option_toggle_action_text": "options.dev_enableDebugHudOverlay.off"
        }
      },
      {
        "@iteratable_toggle_ui.iteratable_toggle": {
          "$this_action": "#debug_hud_radio_basic",
