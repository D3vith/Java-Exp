    "size": ["310px", "100%c"],
    "enabled": "(not $is_publish)",
    "controls": [
      { "vtop@content_ui.v2px": {} },
      {
        "stack0@content_ui.hstack": {
          "controls": [
            { "0@debug_ui.enable_debug_ui_toggle": {} },
            { "hfill@content_ui.hfill": {} },
            { "1@debug_ui.default_font_overrides_toggle": {} }
          ]
        }
      },
      { "space0@content_ui.v4px": {} },
      {
        "stack0@content_ui.hstack": {
          "controls": [
            { "0@debug_ui.render_bounding_box_toggle": {} },
            { "hfill@content_ui.hfill": {} },
            { "1@debug_ui.dev_find_mobs_toggle": {} }
          ]
        }
      },
      { "space1@content_ui.v4px": {} },
      {
        "stack1@content_ui.hstack": {
          "controls": [
            { "0@debug_ui.dev_render_attach_pos_slider": {} },
            { "hfill@content_ui.hfill": {} },
