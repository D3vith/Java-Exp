      { "space3@content_ui.v4px": {} },
      {
        "stack3@content_ui.hstack": {
          "controls": [
            { "0@debug_ui.dev_show_chunk_map_toggle": {} },
            { "hfill@content_ui.hfill": {} },
            { "1@debug_ui.dev_render_mob_info_state_toggle": {} }
          ]
        }
      },
      { "space4@content_ui.v4px": {} },
      {
