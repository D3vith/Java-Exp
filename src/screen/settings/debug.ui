  "bottom_content": {
    "type": "panel",
    "controls": [
      { "flighting_debug@settings_ui.open_button": { "offset": ["50% - 155px", 6], "enabled": "(not $is_publish)", "$toggle_text": "options.flighting_debug" } },
      { "back@settings_ui.back_button": { "offset": ["50% + 5px", 6], "size": [150, 20] } }
    ]
  },

  // Widgets

  // Enable when the debug UI option is added
  "enable_debug_ui_toggle@option_toggle_ui.toggle": {
    "$widget_text": "options.dev_enableDebugUI",
    "$option_binding_name": "#dev_enable_debug_ui",
    "$option_enabled_binding_name": "#dev_enable_debug_ui_enabled",
    "$toggle_name": "dev_enable_debug_ui"
