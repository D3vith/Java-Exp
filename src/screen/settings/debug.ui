  "dev_connection_quality_iteratable_toggle@iteratable_toggle_ui.iteratable_toggle_panel": {
    "size": [310, 20],
    "$widget_text": "options.dev_connection_quality",
    "$option_enabled_binding_name": "#dev_connection_quality_enabled",
    "$options": [
      {
        "@iteratable_toggle_ui.iteratable_toggle": {
          "$this_action": "#dev_connection_off",
          "$next_action": "#dev_connection_nolimit",
          "$option_toggle_action_text": "options.dev_connection_off",
          "variables": {
            "requires": "$is_publish",
            "$option_toggle_action_text": "-"
          }
        }
      },
      {
        "@iteratable_toggle_ui.iteratable_toggle": {
          "$this_action": "#dev_connection_nolimit",
          "$next_action": "#dev_connection_4g",
          "$option_toggle_action_text": "options.dev_connection_nolimit",
          "variables": {
            "requires": "$is_publish",
            "$option_toggle_action_text": "-"
          }
        }
      },
      {
        "@iteratable_toggle_ui.iteratable_toggle": {
          "$this_action": "#dev_connection_4g",
          "$next_action": "#dev_connection_3g",
          "$option_toggle_action_text": "options.dev_connection_4g",
          "variables": {
            "requires": "$is_publish",
            "$option_toggle_action_text": "-"
          }
        }
      },
      {
        "@iteratable_toggle_ui.iteratable_toggle": {
          "$this_action": "#dev_connection_3g",
          "$next_action": "#dev_connection_slow",
          "$option_toggle_action_text": "options.dev_connection_3g",
          "variables": {
            "requires": "$is_publish",
            "$option_toggle_action_text": "-"
          }
        }
      },
      {
        "@iteratable_toggle_ui.iteratable_toggle": {
          "$this_action": "#dev_connection_slow",
          "$next_action": "#dev_connection_veryslow",
          "$option_toggle_action_text": "options.dev_connection_slow",
          "variables": {
            "requires": "$is_publish",
            "$option_toggle_action_text": "-"
          }
        }
      },
      {
        "@iteratable_toggle_ui.iteratable_toggle": {
          "$this_action": "#dev_connection_veryslow",
          "$next_action": "#dev_connection_off",
          "$option_toggle_action_text": "options.dev_connection_veryslow",
          "variables": {
            "requires": "$is_publish",
            "$option_toggle_action_text": "-"
          }
        }
      }
    ]
  },

