{
  "namespace": "debug_ui",

  "debug_screen_panel@content_ui.scroll_screen_panel": {
    "$top_content": "debug_ui.top_content",
    "$middle_content": "debug_ui.debug_content",
    "$bottom_content": "debug_ui.bottom_content",
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "debug_view",
        "source_property_name": "#toggle_state",
        "target_property_name": "#visible"
      }
    ]
  },

  "top_content": {
    "type": "panel",
    "controls": [ { "title@content_ui.title": { "text": "options.debug", "offset": ["50% - 50%x", 15] } } ]
  },

