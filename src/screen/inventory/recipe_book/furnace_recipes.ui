  // Show on furnace

  "show_on_panel": {
    "type": "panel",
    "size": [82, 54],
    "anchor_to": "top_right",
    "anchor_from": "top_right",
    "offset": ["100%x + 57px", 16],
    "controls": [
      {
        "input": {
          "type": "panel",
          "size": [18, 18],
          "anchor_to": "top_left",
          "anchor_from": "top_left",
          "controls": "$input_controls"
        }
      },
      {
        "output": {
          "type": "panel",
          "size": [18, 18],
          "anchor_to": "top_right",
          "anchor_from": "top_right",
          "offset": [-4, 18],
          "$slotbg_size": ["100% - 2px + 8px", "100% - 2px + 8px"],
          "controls": "$output_controls"
        }
      },
      {
        "show_fuel@furnace_recipes_ui.show_fuel": {
          "anchor_to": "bottom_left",
          "anchor_from": "bottom_left",
          "$slotbg_size": ["100% - 2px + 2px", "100% - 2px + 2px"],
          "offset": [1, -1]
        }
      }
    ]
  },

  "show_item": {
    "type": "panel",
    "size": [18, 18],
    "layer": 0,
    "controls": [
      { "red@furnace_recipes_ui.red_background": {} },
      { "white@furnace_recipes_ui.white_overlay": {} },
      { "item_renderer@furnace_recipes_ui.item_renderer": {} }
    ],
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "($furnace_type + '_' + $tab_source + '_book_slot_' + $item + '_view')",
        "source_property_name": "#toggle_state",
        "target_property_name": "#visible"
      }
    ]
  },

